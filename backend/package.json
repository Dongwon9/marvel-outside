{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "NODE_ENV=test jest --config jest.integration.cjs",
    "test:integration:watch": "NODE_ENV=test jest --config jest.integration.cjs --watch",
    "test:all": "pnpm test && pnpm test:integration",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:push:dev": "NODE_ENV=development prisma db push",
    "db:reset:dev": "NODE_ENV=development prisma migrate reset --force",
    "db:seed": "NODE_ENV=development pnpx tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "pnpx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.2",
